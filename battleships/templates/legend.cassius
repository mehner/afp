div.legend
  background-color: #015965
  color: #EEE
  border: 1px solid black
  height: 100%

div.legendCaption
  font-weight: bold
  font-size: 20px
  margin: 5px

table.legend
  text-align: left
  border-spacing: 0px
  border-collapse: separate

table.legend td
  padding: 5px
  vertical-align: middle
  color: #EEE

table.legend tr:nth-child(odd)
  background-color: #03899C

table.legend tr:nth-child(even)
  background-color:#015965